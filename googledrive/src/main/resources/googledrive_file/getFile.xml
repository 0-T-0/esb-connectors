<template name="getFile" xmlns="http://ws.apache.org/ns/synapse">

	<parameter name="fileId" description="ID of the file" />
	<parameter name="updateViewedDate"
		description="Whether or not to update the last viewed date" />

	<sequence>
	
		<property name="uri.var.fileId" expression="$func:fileId" />
		<property name="uri.var.updateViewedDate" expression="$func:updateViewedDate" />

		<class name="org.wso2.carbon.connector.googledrive.GoogledriveGetfile" />

	</sequence>
</template>
