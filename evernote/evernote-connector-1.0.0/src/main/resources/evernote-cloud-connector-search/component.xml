<?xml version="1.0" encoding="UTF-8"?>
<component name="evernote-cloud-connector-search" type="synapse/template">

    <subComponents>
        <!--Search-->
        <component name="createSearch">
            <file>createSearch.xml</file>
            <description>Makes a new SavedSearch in the account </description>
        </component>
        <component name="expungeSearch">
            <file>expungeSearch.xml</file>
            <description>Permanently removes an existing SavedSearch</description>
        </component>
        <component name="getSearch">
            <file>getSearch.xml</file>
            <description>Retrieve an existing SavedSearch</description>
        </component>
        <component name="updateSearch">
            <file>updateSearch.xml</file>
            <description>Update an existing SavedSearch</description>
        </component>
    </subComponents>

</component>
