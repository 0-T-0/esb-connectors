<component name="LiveBroadcasts" type="synapse/template">
    <subComponents>
        <component name="getLiveBroadcasts">
            <file>getLiveBroadcasts.xml</file>
            <description>Returns a list of YouTube broadcasts that match the API request parameters</description>
        </component>
        <component name="insertLiveBroadcasts">
            <file>insertLiveBroadcasts.xml</file>
            <description>Creates a broadcast</description>
        </component>
        <component name="updateLiveBroadcasts">
            <file>updateLiveBroadcasts.xml</file>
            <description>Updates a broadcast, you could modify the broadcast settings defined in the liveBroadcast
                resource's contentDetails object</description>
        </component>
        <component name="bindLiveBroadcasts">
            <file>bindLiveBroadcasts.xml</file>
            <description>Binds a YouTube broadcast to a stream or removes an existing binding between a broadcast and a stream</description>
        </component>
        <!--<component name="transitionLiveBroadcasts">-->
