            <!--<file>controlLiveBroadcasts.xml</file>-->
            <!--<description>Controls the settings for a slate that can be displayed in the broadcast stream</description>-->
        <!--</component>-->
        <component name="deleteLiveBroadcasts">
            <file>deleteLiveBroadcasts.xml</file>
            <description>Deletes a broadcast</description>
        </component>
    </subComponents>
</component>
