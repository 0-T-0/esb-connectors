<?xml version="1.0" encoding="UTF-8"?>
<template xmlns="http://ws.apache.org/ns/synapse" name="init">
    <parameter name="apiKey"/>
    <parameter name="accessToken"/>
    <sequence>
         <property name="uri.var.api_key"      expression="$func:apiKey"      scope="default" type="STRING"/>
         <property name="uri.var.api_version"  value="3"                      scope="default" type="STRING"/>
         <property name="uri.var.access_token" expression="$func:accessToken" scope="default" type="STRING"/>
    </sequence>
</template>
