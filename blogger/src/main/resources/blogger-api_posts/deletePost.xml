<?xml version="1.0" encoding="UTF-8"?>
<template xmlns="http://ws.apache.org/ns/synapse" name="deletePost">
     <parameter name="blogID"/>
     <parameter name="postID"/>
<sequence>
        <property expression="$func:blogID"  name="uri.var.blog_id"  scope="default" type="STRING"/>
        <property expression="$func:postID"  name="uri.var.post_id"  scope="default" type="STRING"/>
        <property action="remove" name="Accept-Encoding" scope="transport"/>
        <payloadFactory media-type="json">
            <format>
              { }
            </format>
                        <args></args>
        </payloadFactory>
         <property name="messageType" scope="axis2" value="application/json"/>
         <call>
        	<endpoint>
        	<http method="delete" uri-template="https://www.googleapis.com/blogger/v{uri.var.api_version}/blogs/{uri.var.blog_id}/posts/{uri.var.post_id}?access_token&#61;{uri.var.access_token}"></http>
            </endpoint>
         </call>
    </sequence>
    <header name="cache-control" scope="transport" action="remove" />
 <header name="content-encoding" scope="transport" action="remove" />
 <header name="content-length" scope="transport" action="remove" />
 <header name="content-type" scope="transport" action="remove" />
 <header name="date" scope="transport" action="remove" />
 <header name="expires" scope="transport" action="remove" />
 <header name="server" scope="transport" action="remove" />
  <header name="etag" scope="transport" action="remove" />
 
</template>
