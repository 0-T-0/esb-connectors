<?xml version="1.0" encoding="UTF-8"?>
<proxy xmlns="http://ws.apache.org/ns/synapse" name="publishPost"
	transports="https,http" statistics="disable" trace="disable"
	startOnLoad="true">
	<target>
		<inSequence>
			<property name="apiKey" expression="json-eval($.apiKey)" />
			<property name="accessToken" expression="json-eval($.accessToken)" />
			<property expression="json-eval($.blogID)" name="blogID" />
			<property expression="json-eval($.postID)" name="postID" />
			<property expression="json-eval($.publishDate)" name="publishDate" />
			<property expression="json-eval($.fields)" name="fields" />
			<blogger.init>
				<apiKey>{$ctx:apiKey}</apiKey>
				<accessToken>{$ctx:accessToken}</accessToken>
			</blogger.init>
			<blogger.publishPost>
				<blogID>{$ctx:blogID}</blogID>
				<postID>{$ctx:postID}</postID>
				<publishDate>{$ctx:publishDate}</publishDate>
				<fields>{$ctx:fields}</fields>
			</blogger.publishPost>
			
			<respond />
		</inSequence>
		<outSequence>
			<log />
			<send />
		</outSequence>
	</target>
	<description />
</proxy>