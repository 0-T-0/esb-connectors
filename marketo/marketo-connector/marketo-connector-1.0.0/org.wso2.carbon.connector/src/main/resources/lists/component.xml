<?xml version="1.0" encoding="UTF-8"?>
<component name="leads" type="synapse/template">
    <subComponents>
        <component name="getListById">
            <file>getListById.xml</file>
            <description>Retrieves a list for a given Marketo Id</description>
        </component>
        <component name="getMultipleLists">
            <file>getMultipleLists.xml</file>
            <description>Retrieves multiple lists</description>
        </component>
        <component name="addLeadsToList">
            <file>addLeadsToList.xml</file>
            <description>Add leads to a list</description>
        </component>
        <component name="memberOfList">
            <file>memberOfList.xml</file>
            <description>Finds out if leads are a member of a given list.</description>
        </component>
        <component name="removeLeadsFromList">
            <file>removeLeadsFromList.xml</file>
            <description>Removes a lead from a given list.</description>
        </component>
    </subComponents>
</component>