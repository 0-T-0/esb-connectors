<definitions xmlns="http://ws.apache.org/ns/synapse">
    <proxy xmlns="http://ws.apache.org/ns/synapse"
           name="rmsend"
           transports="https,http"
           statistics="disable"
           trace="disable"
           startOnLoad="true">
        <target>
            <inSequence>
                <reliable-message.send>
                    <wsdl>http://localhost:9010/SoapContext/GreeterPort?wsdl</wsdl>
                    <service>GreeterService</service>
                    <port>GreeterPort</port>
                    <baseRetransmissionInterval>4000</baseRetransmissionInterval>
                    <acknowledgementInterval>2000</acknowledgementInterval>
                    <intraMessageThreshold>0</intraMessageThreshold>
                    <dynamicParameters>false</dynamicParameters>
                </reliable-message.send>
                <respond/>
            </inSequence>
            <outSequence>
                <send/>
            </outSequence>
            <faultSequence>
                <log level="custom">
                    <property name="text" value="An unexpected error occured"/>
                    <property name="message" expression="get-property('ERROR_MESSAGE')"/>
                </log>
                <drop/>
            </faultSequence>
        </target>
        <description/>
    </proxy>
</definitions>



