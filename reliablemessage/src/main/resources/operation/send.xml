<?xml version="1.0" encoding="UTF-8"?>
<template xmlns="http://ws.apache.org/ns/synapse" name="send">
    <parameter name="wsdl" description="WSDL URL"/>
    <parameter name="port" description="Port name"/>
    <parameter name="service" description="Service Name"/>
    <parameter name="baseRetransmissionInterval" description="BaseRetransmission Interval"/>
    <parameter name="acknowledgementInterval" description="Acknowledgement Interval"/>
    <parameter name="intraMessageThreshold" description="IntraMessage Threshold"/>
    <parameter name="dynamicParameters" description="Boolean indicator for the parameter value type"/>

<sequence>
    <property name="rm.wsdl" expression="$func:wsdl" />
    <property name="rm.port" expression="$func:port" />
    <property name="rm.service" expression="$func:service" />
    <property name="rm.baseRetransmissionInterval" expression="$func:baseRetransmissionInterval" />
    <property name="rm.acknowledgementInterval" expression="$func:acknowledgementInterval" />
    <property name="rm.intraMessageThreshold" expression="$func:intraMessageThreshold" />
    <property name="rm.dynamicParameters" expression="$func:dynamicParameters" />
    <class name="org.wso2.carbon.connector.rm.ReliableMessage" />
</sequence>
</template>
