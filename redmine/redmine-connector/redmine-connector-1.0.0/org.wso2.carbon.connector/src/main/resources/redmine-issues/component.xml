<?xml version="1.0" encoding="UTF-8"?>
<component name="redmine-issues" type="synapse/template">
  <subComponents>
    <component name="listIssues">
      <file>listIssues.xml</file>
      <description>Returns a paginated list of issues.   </description>
    </component>
    <component name="getIssue">
      <file>getIssue.xml</file>
      <description>Gets details of a single issue by its ID.</description>
    </component>
    <component name="createIssue">
      <file>createIssue.xml</file>
      <description>Creates a new issue.</description>
    </component>
    <component name="updateIssue">
      <file>updateIssue.xml</file>
      <description>Updates an existing issue.</description>
    </component>
    <component name="deleteIssue">
      <file>deleteIssue.xml</file>
      <description>Deletes an existing issue.</description>
    </component>
  </subComponents>
</component>