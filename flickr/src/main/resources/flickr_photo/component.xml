<?xml version="1.0" encoding="UTF-8"?>
<component name="flickr_photo" type="synapse/template">
	<subComponents>
		<component name="getPhotoInfo">
			<file>getPhotoInfo.xml</file>
			<description>get information of the photo.</description>
		</component>
        <component name="getExif">
			<file>getExif.xml</file>
			<description>get Exif information of the photo.</description>
		</component>
        <component name="addTags">
			<file>addTags.xml</file>
			<description>add a tag to a photo.</description>
		</component>
        <component name="removeTag">
			<file>removeTag.xml</file>
			<description>remove a tag from a photo.</description>
		</component>
        <component name="deletePhoto">
			<file>deletePhoto.xml</file>
			<description>delete a photo.</description>
		</component>
	</subComponents>
</component>