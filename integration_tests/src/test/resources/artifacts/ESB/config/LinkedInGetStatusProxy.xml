<?xml version="1.0" encoding="UTF-8"?>
<proxy xmlns="http://ws.apache.org/ns/synapse" name="linkedin_getStatus_proxy_NS"
	transports="https,http" statistics="disable" trace="disable"
	startOnLoad="true">
	<target>
		<inSequence>
			<log level="full">
				<property xmlns:ns="wso2.connector.linkedin.getstatus"
					name="accessToken" expression="//ns:accessToken/text()" />
				<property xmlns:ns="wso2.connector.linkedin.getstatus"
					name="appUri" expression="//ns:appUri/text()" />
			</log>
			<linkedin.init>
				<accessToken>{$ctx:accessToken}</accessToken>
				<appUri>{$ctx:appUri}</appUri>
			</linkedin.init>
			<linkedin.getstatus />
			<header name="To" action="remove" />
			<property name="NO_ENTITY_BODY" scope="axis2" action="remove" />
			<property name="RESPONSE" value="true" scope="default" type="STRING" />
			<send />
		</inSequence>
	</target>
</proxy>