<?xml version="1.0" encoding="UTF-8"?>
<proxy xmlns="http://ws.apache.org/ns/synapse"
       name="tw_updateNotebookCategory"
       transports="https http"
       startOnLoad="true"
       trace="disable">
   <target>
      <inSequence>
         <property name="apiUrl" expression="json-eval($.apiUrl)"/>
         <property name="apiKey" expression="json-eval($.apiKey)"/>
         <property name="notebookCategoryId" expression="json-eval($.notebookCategoryId)"/>
         <property name="name" expression="json-eval($.name)"/>
         <property name="parentId" expression="json-eval($.parentId)"/>
         <teamwork.init>
            <apiUrl>{$ctx:apiUrl}</apiUrl>
            <apiKey>{$ctx:apiKey}</apiKey>
         </teamwork.init>
         <teamwork.updateNotebookCategory>
            <notebookCategoryId>{$ctx:notebookCategoryId}</notebookCategoryId>
            <name>{$ctx:name}</name>
            <parentId>{$ctx:parentId}</parentId>
         </teamwork.updateNotebookCategory>
         <respond/>
      </inSequence>
      <outSequence/>
      <faultSequence/>
   </target>
</proxy>
