<?xml version="1.0" encoding="UTF-8"?>
<proxy xmlns="http://ws.apache.org/ns/synapse"
       name="tw_updateCurrentUserStatus"
       transports="https http"
       startOnLoad="true"
       trace="disable">
   <description/>
   <target>
      <inSequence>
         <property name="apiUrl" expression="json-eval($.apiUrl)"/>
         <property name="apiKey" expression="json-eval($.apiKey)"/>
         <property name="statusId" expression="json-eval($.statusId)"/>
         <property name="status" expression="json-eval($.status)"/>
         <property name="notify" expression="json-eval($.notify)"/>
         <teamwork.init>
            <apiUrl>{$ctx:apiUrl}</apiUrl>
            <apiKey>{$ctx:apiKey}</apiKey>
         </teamwork.init>
         <teamwork.updateCurrentUserStatus>
            <statusId>{$ctx:statusId}</statusId>
            <status>{$ctx:status}</status>
            <notify>{$ctx:notify}</notify>
         </teamwork.updateCurrentUserStatus>
         <respond/>
      </inSequence>
      <outSequence/>
      <faultSequence/>
   </target>
</proxy>
