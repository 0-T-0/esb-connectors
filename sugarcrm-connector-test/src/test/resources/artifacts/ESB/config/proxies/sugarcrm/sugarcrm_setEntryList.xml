<?xml version="1.0" encoding="UTF-8"?>
<proxy xmlns="http://ws.apache.org/ns/synapse"
       name="sugarcrm_setEntryList"
       transports="https,http"
       statistics="disable"
       trace="disable"
       startOnLoad="true">
   <target>
      <inSequence>
         <property xmlns:ns="wso2.connector.sugarcrm.setentry"
                   name="sessionId"
                   expression="//ns:sessionId/text()"/>
         <property xmlns:ns="wso2.connector.sugarcrm.setentry"
                   name="appUri"
                   expression="//ns:sugarcrmUrl/text()"/>
         <property xmlns:ns="wso2.connector.sugarcrm.setentry"
                   name="module"
                   expression="//ns:moduleName/text()"/>
         <property xmlns:ns="wso2.connector.sugarcrm.setentry"
                   name="nameValueLists"
                   expression="//ns:nameValueLists"/>
         <sugarcrm-connector-1.0.0.init>
            <appUri>{$ctx:appUri}</appUri>
            <session>{$ctx:sessionId}</session>
         </sugarcrm-connector-1.0.0.init>
         <sugarcrm-connector-1.0.0.setEntryList>
            <moduleName>{$ctx:module}</moduleName>
            <nameValueLists>{$ctx:nameValueLists}</nameValueLists>
         </sugarcrm-connector-1.0.0.setEntryList>
         <respond/>
      </inSequence>
      <outSequence>
         <log/>
         <send/>
      </outSequence>
   </target>
   <description/>
</proxy>
