<template xmlns="http://ws.apache.org/ns/synapse" name="init">
	<parameter name="uri"
		description="password of the user used to log in to JIRA." />
    <parameter name="consumerKey"/>
    <parameter name="consumerSecret"/>
    <parameter name="accessToken"/>
    <parameter name="tokenSecret"/>
	<sequence>
		<property name="uri.var.jira.url" expression="$func:uri" />
        <header name="Consumer Key" expression="$func:consumerKey"
                scope="transport"/>
        <header name="Consumer Secret" expression="$func:consumerSecret"
                scope="transport"/>
        <header name="Access Token" expression="$func:accessToken"
                scope="transport"/>
        <header name="Token Secret" expression="$func:tokenSecret"
                scope="transport"/>
	</sequence>
</template>
